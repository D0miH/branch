{
    "name": "branch",
    "productName": "Branch",
    "version": "0.1.0",
    "private": true,
    "main": "build/electron.js",
    "homepage": "./",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "dev": "yarn build-electron && concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
        "build-electron": "./node_modules/.bin/tsc ./public/electron.ts --outDir ./build/",
        "electron-pack": "build --em.main=build/electron.js",
        "pack": "react-scripts build && yarn build-electron && electron-builder --dir",
        "dist": "react-scripts build && yarn build-electron && electron-builder"
    },
    "eslintConfig": {
        "extends": "react-app"
       
    },
    "prettier": {
        "tabWidth": 4,
        "semi": true
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "dependencies": {
        "@githubprimer/octicons-react": "^8.5.0",
        "@material-ui/core": "^3.9.2",
        "@material-ui/icons": "^3.0.2",
        "@types/electron-is-dev": "^0.3.0",
        "@types/jest": "^24.0.9",
        "@types/node": "^11.9.5",
        "@types/react": "^16.8.5",
        "@types/react-dom": "^16.8.2",
        "electron-is-dev": "^1.0.1",
        "mobx": "^5.9.0",
        "mobx-react": "^5.4.3",
        "nodegit": "^0.24.1",
        "react": "^16.8.3",
        "react-dom": "^16.8.3",
        "react-scripts": "2.1.5",
        "tslint": "^5.13.1"
    },
    "devDependencies": {
        "concurrently": "^4.1.0",
        "electron": "^4.0.5",
        "electron-builder": "^20.38.5",
        "typescript": "^3.3.3333",
        "wait-on": "^3.2.0"
    },
    "build": {
        "appId": "D0miH.branch",
        "mac": {
            "category": "public.app-category.developer-tools",
            "identity": null
        },
        "files": [
            "./build/**/*",
            "./build/electron.js"
        ],
        "extends": null
    }
}
